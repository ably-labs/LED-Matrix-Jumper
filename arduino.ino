#include <Adafruit_NeoPixel.h>
#include <ESP8266WiFi.h>
#ifdef __AVR__
#include <avr/power.h>
#endif

#define PIN 4
#define NUM_LIGHTS  256

Adafruit_NeoPixel strip = Adafruit_NeoPixel(NUM_LIGHTS, PIN, NEO_GRB + NEO_KHZ800);
typedef struct { String key; int value[256][3]; } key_value_pair;

void setup() {
  strip.begin();
  strip.show();
  Serial.begin(115200);
  delay(100);
}

void loop() {
  Serial.print("Tick");
  
  connect_wifi();
  
  auto image_key = call_api();
  if (image_key.equals("")) {
    delay(1000);
    return;
  }

  auto image_record = value_or_default(image_key);
  update_lights(image_record);
  strip.show();
  
  delay(1000);
}

void connect_wifi() {
  const char* ssid     = "your-ssid";
  const char* password = "your-wifi-password!";

  Serial.print("Connecting to ");
  Serial.println(ssid);

  WiFi.begin(ssid, password);

  while (WiFi.status() != WL_CONNECTED) {
    delay(500);
    Serial.print(".");
  }
  Serial.println("");
  Serial.println("WiFi connected");
}

String call_api() {
  Serial.println("calling api");

  const char* host = "your-dns-name-where-app-is-running-on-port-80";

  WiFiClient client;
  const int httpPort = 80;
  if (!client.connect(host, httpPort)) {
    Serial.println("connection failed");
    return "";
  }

  // We now create a URI for the request
  String url = "/active-image";

  // This will send the request to the server
  client.print(String("GET ") + url + " HTTP/1.1\r\n" + "Host: " + host + "\r\n" + "Connection: close\r\n\r\n");
  delay(500);

  // Read one line of the reply from server
  String icon;
  while (client.available()) {
    icon = client.readStringUntil('\r');
  }

  Serial.print("API returned: " + icon);
  icon.trim();
  return icon;
}

void update_lights(key_value_pair* kvp) {
  Serial.print("Updating lights");

  for (auto i = 0; i < NUM_LIGHTS; i++) {
    auto rgb = kvp->value[i];
    strip.setPixelColor(i, strip.Color(rgb[0], rgb[1], rgb[2]));
  }

  Serial.println("Colours set");
}

